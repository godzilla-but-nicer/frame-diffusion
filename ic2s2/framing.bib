	publisher = {ACM},
	author = {Hauffa, Jan and Bräu, Wolfgang and Groh, Georg},
	month = aug,
	year = {2019},
	pages = {969--977},
	file = {Hauffa et al. - 2019 - Detection of topical influence in social networks .pdf:/home/patgwall/snap/zotero-snap/common/Zotero/storage/UGMPAYD7/Hauffa et al. - 2019 - Detection of topical influence in social networks .pdf:application/pdf},
}

